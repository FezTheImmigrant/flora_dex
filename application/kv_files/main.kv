<RoundedButton@Button>
    background_normal: ''
    background_color:(0,0,0,0)
    canvas.before:
        Color:
            rgba: (0.678, 0.917, 0.603,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [10]
